    <%= javascript_include_tag "admin_20141208/js/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js"%>
    <%= javascript_include_tag "admin_20141208/js/plugins/iCheck/icheck.min.js"%>
    <%= javascript_include_tag "admin_20141208/js/AdminLTE/app.js"%>
    <%#= javascript_include_tag "admin_20141208/js/AdminLTE/dashboard.js" %>
