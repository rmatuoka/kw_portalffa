        <a class="navbar-btn sidebar-toggle" data-toggle="offcanvas" href="#" role="button"></a>
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
    </header>
    <div class="wrapper row-offcanvas row-offcanvas-left">
      <aside class="left-side sidebar-offcanvas">
        <%=render :file => "layouts/_menu_admin_20141208.html.erb"%>
      </aside>
      
      <aside class="right-side">
        <%=yield%>
      </aside>
      
    </div>
    <%= javascript_include_tag :defaults %>
    <%= javascript_include_tag "admin_20141208/js/jquery-ui-1.10.3.min.js"%>
    <%= javascript_include_tag "admin_20141208/js/bootstrap.min.js"%>
    <%= javascript_include_tag "admin_20141208/js/raphael-min.js"%>
    <%#= javascript_include_tag "admin_20141208/js/plugins/morris/morris.min.js"%>
