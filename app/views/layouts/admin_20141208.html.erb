    <!-- Bootstrap -->   
    <%= stylesheet_link_tag "admin_20141208/css/bootstrap.min.css"%>
    <%= stylesheet_link_tag "admin_20141208/css/font-awesome.min.css"%> 
    <%= stylesheet_link_tag "admin_20141208/css/ionicons.min.css"%> 
    <%= stylesheet_link_tag "admin_20141208/css/morris/morris.css"%> 
    <%= stylesheet_link_tag "admin_20141208/css/jvectormap/jquery-jvectormap-1.2.2.css"%> 
