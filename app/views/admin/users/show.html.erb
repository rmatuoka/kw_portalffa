<% 
	content_for :title do "Dados do Usuario" end
%>

<p>
  <strong>Nome:</strong>
  <%= @user.name %>
</p>
<p>
  <strong>Sobrenome:</strong>
  <%= @user.last_name %>
</p>
<p>
  <strong>E-mail:</strong>
  <%= @user.email %>
</p>
<p>
  <%= link_to t('attributes.editing'), edit_admin_user_path(@user) %> |
  <%= link_to t('attributes.exclude'), admin_user_path(@user), :confirm => 'Esta operação não poderá ser desfeita, deseja continuar?', :method => :delete %> |
  <%= link_to "View All", admin_users_path %>
</p>
