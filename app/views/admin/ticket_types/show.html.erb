<% title @ticket_type.name %>
<section class="content-header">
  <h1>
    <%= @ticket_type.name %>
    <br/>
    <small><%= t('activerecord.attributes.ticket_type.messages.show')%></small>
  </h1>
  <ol class="breadcrumb">
    <li>
      <a href="/admin">
        <i class="fa fa-dashboard"></i>
        Home
      </a>
    </li>
    <li class="active"> <%= show_model_name(::TicketType, true) %> </li> 
  </ol>
</section>

<section class="content">
  <div class="box">
    <div class="box-body">
      <p>
        <strong><%=t 'activerecord.attributes.ticket_type.name' %>:</strong>
        <%= @ticket_type.name %>
      </p>
      <p>
        <strong><%=t 'activerecord.attributes.ticket_type.discount' %>:</strong>
        <%= number_to_currency(@ticket_type.discount) %>
      </p>
      <p>
        <strong><%=t 'activerecord.attributes.ticket_type.published' %>:</strong>
        <%= print_active(@ticket_type.published) %>
      </p>
      <br />
      <p class="box-footer">
          <%= submit_tag t('attributes.go_back'),:type=>"button",:onclick=>"window.open('"+admin_ticket_types_path+"','_self');", :class => "btn btn-default", :style=>"margin-right: 5px;"%>
      </p>    
    </div>    
  </div>
</section>


