<% title "Admin/Ticket Types" %>

<table>
  <tr>
    <th>Name</th>
    <th>Discount</th>
    <th>Active</th>
    <th>Published</th>
  </tr>
  <% for ticket_type in @ticket_types %>
    <tr>
      <td><%= ticket_type.name %></td>
      <td><%= ticket_type.discount %></td>
      <td><%= ticket_type.active %></td>
      <td><%= ticket_type.published %></td>
      <td><%= link_to "Show", [:admin, ticket_type] %></td>
      <td><%= link_to "Edit", edit_admin_ticket_type_path(ticket_type) %></td>
      <td><%= link_to "Destroy", [:admin, ticket_type], :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Admin/Ticket Type", new_admin_ticket_type_path %></p>
