<%= form_for ([:admin, @banner], :html => { :multipart => :true }) do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p>
    <%= f.label :link %><br />
    <%= f.text_field :link %>
  </p>
  <p>
    <%= f.label :link_target %><br />
    <%= f.text_field :link_target %>
  </p>
	<p>
	    <%= f.label :file %><br />
	    <%= f.file_field :file %>
	  </p>
	<p>
	    <%= f.label :published %><br />
	    <%= f.check_box :published %>
	  </p>
  <p><%= f.submit %></p>
<% end %>
