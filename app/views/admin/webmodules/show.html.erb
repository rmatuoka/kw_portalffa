<% title @template.name+" / "+ @webmodule.webpart.name%>
<section class="content-header">
  <h1>
    <%= @template.name+"  / "+ @webmodule.webpart.name %>
    <br/>
    <small><%= t('activerecord.attributes.webmodule.messages.show')%></small>
  </h1>
  <ol class="breadcrumb">
    <li>
      <a href="/admin">
        <i class="fa fa-dashboard"></i>
        Home
      </a>
    </li>
    <li class="active"> <%= show_model_name(::Webmodule, true)%> </li> 
  </ol>
</section>

<section class="content">
  <div class="box">
    <div class="box-header">
      <h3 class="box-title">
      
      </h3>
    </div>
    <div class="box-body">
      <p>
        <strong><%=show_model_name(::Webpart)+" "+t('attributes.name') %>:</strong>
        <%= @webmodule.webpart.name %>
      </p>
      <p>
        <strong><%=t 'activerecord.attributes.webmodule.position' %>:</strong>
        <%= @webmodule.position %>
      </p>
      <p>
        <strong><%=t 'activerecord.attributes.webmodule.module_key' %>:</strong><br />
        <%= @webmodule.module_key %>
      </p>
      <p>
        <strong><%=t 'attributes.published' %>:</strong>
        <%= print_active(@webmodule.published) %>
      </p>
      <br />
      <p class="box-footer">
        <%= submit_tag t('attributes.go_back'),:type=>"button",:onclick=>"window.open('"+admin_template_webmodules_path(@template)+"','_self');", :class => "btn btn-default", :style=>"margin-right: 5px;"%>
      </p>
    </div>    
  </div>
</section>




