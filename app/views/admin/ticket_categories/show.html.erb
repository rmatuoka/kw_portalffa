<% title @ticket_category.name %>
<section class="content-header">
  <h1>
    <%= @ticket_category.name %>
    <br/>
    <small><%= t('activerecord.attributes.ticket_category.messages.show')%></small>
  </h1>
  <ol class="breadcrumb">
    <li>
      <a href="/admin">
        <i class="fa fa-dashboard"></i>
        Home
      </a>
    </li>
    <li class="active"> <%= show_model_name(::TicketCategory, true) %> </li> 
  </ol>
</section>
<section class="content">
  <div class="box">
    <div class="box-body">
      <p>
        <strong><%=t 'activerecord.attributes.ticket_category.name' %>:</strong>
        <%= @ticket_category.name %>
      </p>
      <p>
        <strong><%=t 'activerecord.attributes.ticket_category.order' %>:</strong>
        <%= @ticket_category.order %>
      </p>
      <p>
        <strong><%=t 'activerecord.attributes.ticket_category.published' %>:</strong>
        <%= print_active(@ticket_category.published) %>
      </p>
      <br />
      <p class="box-footer">
	      <%= submit_tag t('attributes.go_back'),:type=>"button",:onclick=>"window.open('"+admin_ticket_categories_path+"','_self');", :class => "btn btn-default", :style=>"margin-right: 5px;"%>
      </p>
    </div>    
  </div>
</section>


