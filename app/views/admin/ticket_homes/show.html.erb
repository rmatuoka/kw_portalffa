<% title show_model_name(::TicketHome) %>
<section class="content-header">
  <h1>
    <% show_model_name(::TicketHome) %>
    <br/>
    <small><%= t('activerecord.attributes.ticket_home.messages.show')%></small>
  </h1>
  <ol class="breadcrumb">
    <li>
      <a href="/admin">
        <i class="fa fa-dashboard"></i>
        Home
      </a>
    </li>
    <li class="active"> <% show_model_name(::TicketHome) %> </li> 
  </ol>
</section>

<section class="content">
  <div class="box">
    <div class="box-body">
      <p>
        <strong><%=t 'activerecord.attributes.ticket_home.name' %>:</strong>
        <%= @ticket_home.name %>
      </p>
      <p>
        <strong><%=t 'activerecord.attributes.ticket_home.content' %>:</strong>
      </p>
      <div class="div_show_textarea">
        <%=raw @ticket_home.content %>	
      </div>	
      <br />
      <p class="box-footer">
	      <%= submit_tag t('attributes.go_back'),:type=>"button",:onclick=>"window.open('"+admin_root_path+"','_self');", :class => "btn btn-default", :style=>"margin-right: 5px;"%>
	      <%= submit_tag t('attributes.edit'),:type=>"button",:onclick=>"window.open('"+edit_admin_ticket_home_path+"','_self');", :class => "btn btn-default", :style=>"margin-right: 5px;"%>
        <%= link_to t('attributes.exclude'), admin_ticket_home_path(@ticket_home), :confirm => t('system_messages.notice_to_delete'), :method => :delete , :class => "btn btn-primary"%>
      </p>
      <div class="clear"></div>
    </div>    
  </div>
</section>


