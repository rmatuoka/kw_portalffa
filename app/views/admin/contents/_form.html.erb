<%= form_for [:admin, @content] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :subcategory_id %><br />
    <%= f.text_field :subcategory_id %>
  </p>
  <p>
    <%= f.label :template_id %><br />
    <%= f.text_field :template_id %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
    <%= f.label :active %><br />
    <%= f.check_box :active %>
  </p>
  <p>
    <%= f.label :published %><br />
    <%= f.check_box :published %>
  </p>
  <p><%= f.submit %></p>
<% end %>
