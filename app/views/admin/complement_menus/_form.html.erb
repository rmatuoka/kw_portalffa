<div id="Box_form">
<%= form_for [:admin, @complement_menu] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name, :class=>"form-control" %>
  </p>
  <p>
    <%= f.label :destination %><br />
    <%= f.text_field :destination, :class=>"form-control" %>
  </p>
  <p>
    <%= f.label :published %>
    <%= f.check_box :published, :class=>"form-control" %>
  </p>
  <br />
  <p class="box-footer"><%= submit_tag t('attributes.go_back'),:type=>"button",:onclick=>"window.open('"+admin_complement_menus_path+"','_self');", :class => "btn btn-default", :style=>"margin-right: 5px;"%> <%= f.submit nil, :class => "btn btn-default" %></p>
<% end %>
</div>
