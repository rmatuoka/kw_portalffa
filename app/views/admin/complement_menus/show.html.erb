<% title @complement_menu.name %>

<section class="content-header">
  <h1>
    <%= @complement_menu.name %>
    <br/>
    <small><%= t('activerecord.attributes.webpart.messages.show')%></small>
  </h1>
  <ol class="breadcrumb">
    <li>
      <a href="/admin">
        <i class="fa fa-dashboard"></i>
        Home
      </a>
    </li>
    <li class="active"> <%= show_model_name(::ComplementMenu, true) %></li> 
  </ol>
</section>

<section class="content">
  <div class="box">
    <div class="box-body">
      <p>
        <strong><%=t 'attributes.name' %>:</strong>
        <%= @complement_menu.name %>
      </p>
      <p>
        <strong><%=t 'activerecord.attributes.complement_menu.destination' %>:</strong>
        <%= @complement_menu.destination %>
      </p>
      <p>
        <strong><%=t 'attributes.published' %>:</strong>
        <%= print_active(@complement_menu.published) %>
      </p>
      <br />
      <p>
	      <%= submit_tag t('attributes.go_back'),:type=>"button",:onclick=>"window.open('"+admin_complement_menus_path+"','_self');", :class => "btn btn-default", :style=>"margin-right: 5px;"%>
      </p>
    </div>    
  </div>
</section>
