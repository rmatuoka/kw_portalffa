<%= form_for [:admin, @footer_site] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :legend %><br />
    <%= f.text_field :legend %>
  </p>
  <p>
    <%= f.label :destination %><br />
    <%= f.text_field :destination %>
  </p>
  <p>
    <%= f.label :link_target %><br />
    <%= f.text_field :link_target %>
  </p>
  <p>
    <%= f.label :image_file_name %><br />
    <%= f.text_field :image_file_name %>
  </p>
  <p>
    <%= f.label :image_content_type %><br />
    <%= f.text_field :image_content_type %>
  </p>
  <p>
    <%= f.label :image_file_size %><br />
    <%= f.text_field :image_file_size %>
  </p>
  <p>
    <%= f.label :image_updated_at %><br />
    <%= f.datetime_select :image_updated_at %>
  </p>
  <p>
    <%= f.label :active %><br />
    <%= f.check_box :active %>
  </p>
  <p>
    <%= f.label :published %><br />
    <%= f.check_box :published %>
  </p>
  <p><%= f.submit %></p>
<% end %>
