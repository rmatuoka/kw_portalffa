<%= form_for [:admin, @guide_type] do |f| %>
  <%= f.error_messages %>
  <div class="form-group form-group-lg">
    <%= f.label :codigo%>
    <%= f.text_field :code, :class=>"form-control"%>
  </div> 
  <div class="form-group form-group-lg">
    <%= f.label :descricao %><br />
    <%= f.text_field :description, :class=>"form-control" %>
  </div> 
  <div class="form-group form-group-lg">
    <%= link_to(t('attributes.go_back'),admin_guide_types_path, :class => "btn btn-lg btn-primary" )%>
    <%= f.submit nil, :class => "btn btn-lg btn-success" %>
  </div> 
<% end %>
