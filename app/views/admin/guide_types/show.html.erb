<% title @guide_type.code %>
<section class="content-header">
  <h1><%= @guide_type.code %>
    <br/>
    <small><%= t('activerecord.attributes.guide_type.messages.show')%></small>
  </h1>
  <ol class="breadcrumb">
    <li>
      <a href="/admin">
        <i class="fa fa-dashboard"></i>
        Home
      </a>
    </li>
    <li class="active"> <%= show_model_name(::GuideType, true) %> </li> 
  </ol>
</section>

<section class="content">
  <div class="box">
    <div class="box-body">
     <p>
        <strong>Codigo:</strong>
        <%= @guide_type.code %>
      </p>
      <p>
        <strong>Descrição:</strong>
        <%= @guide_type.description %>
      </p>
      <p><%= link_to(t('attributes.go_back'),admin_guide_types_path, :class => "btn btn-lg btn-default" )%></p>
    </div>    
  </div>
</section>



